<h1> Artists </h1>
  <h2> Order By: </h2>

    <%= link_to "Name", order_by_name_artists_path %>
    <%= link_to "Date Added", order_by_date_artists_path %>


    <ul>
    <% @artists.each do |artist| %>
    <% @artist = artist %>
      <li><%=artist.name%>
        <% @artist.photos.each do |photo| %>
        <%= image_tag photo.artist_image %>
        <% end %>
      </li>
    <% end %>
    </ul>
