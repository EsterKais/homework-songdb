$("ul.errors").html("")
<% if @song.errors.any? %>
  <% @song.errors.full_messages.each do |message| %>
    $("ul.errors").append($("<li />").html("<%= message.html_safe %>"))
  <% end %>
<% else %>
  $(".song-show").html("<%= escape_javascript(render 'artists/show.html.erb') %>")
  $("#song-modal").modal("hide")
<% end %>


